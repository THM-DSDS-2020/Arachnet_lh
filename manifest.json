{
  "description": "Arachnet",
  "manifest_version": 2,
  "name": "Arachnet",
  "version": "1.0",
  "background": {
    "scripts": [
      "processor.js",
      "capturer.js",
      "vizualizer.js"
    ]
  },
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "activeTab",
    "tabs",
    "<all_urls>"
  ],
  "browser_action": {
    "browser_style": true,
    "default_icon": {
      "16": "icon/Load.png"
    }
  }
}